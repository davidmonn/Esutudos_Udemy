Algoritmo "Teste_Para"


Var

  quantidade, soma,numero,valores : inteiro

Inicio

     escreva("Quantos numeros serao digitados? : ")
     leia(numero)
     
    soma <- 0
        para quantidade de 1 ate numero faca
           escreva ("Digite um numero: ")
           leia (valores)
           soma <- soma + valores

       fimpara

     escreva("Soma: ",soma)


Fimalgoritmo