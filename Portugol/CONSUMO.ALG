Algoritmo "consumo"



Var

distanciaPerc, combustivelGast, consumoMed: real

Inicio

      escreva("Distancia percorrida: ")
      leia(distanciaPerc)
      
      escreva("Combustivel gasto: ")
      leia(combustivelGast)
      
      consumoMed <- distanciaPerc / combustivelGast
      
      escreva("Consumo medio =", consumoMed:8:3)

Fimalgoritmo